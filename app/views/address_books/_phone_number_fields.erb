<div class="phone-numbers">
  <div class="form-group">
    <div class="container">
      <div class="row">
        <div class="col-md-2 col-sm-2">
          <div class="form-group">
            <%= f.text_field :phone,  :class => 'form-control'%>
          </div>
          <%=f.error_span(:phone) %>
          <div class="form-group">
            <%= link_to_remove_association("Remove phone number", f, {wrapper_class: 'phone-numbers'})%>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>
